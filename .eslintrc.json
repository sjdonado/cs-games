{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base",
    "plugin:solid/recommended"
  ],
  "plugins": [
    "solid"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-param-reassign": 0,
    "new-cap": 0,
    "no-new": 0,
    "import/no-unresolved": 0
  }
}
